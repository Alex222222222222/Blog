{
  "extends": [
    "eslint:recommended",
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:react/jsx-runtime"
  ],
  "plugins": ["unused-imports"],
  "rules": {
    "no-unused-vars": "warn",
    "unused-imports/no-unused-imports": "warn"
  }
}
